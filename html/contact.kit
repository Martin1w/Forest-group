<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Forest Website | Najpiękniejszy park w Twojej okolicy</title>
    <!-- @include 'styles.kit' -->
</head>

<body>
    <!-- @include 'nav.kit' -->



    <section class="contact">
        <div class="contact__hero"></div>
        <div class="wrapper">
            <h2 class="contact-header section-heading">Kontakt</h2>

            <div class="form">


                <div class="form-text">
                    <label for="name">Imię</label>
                    <input type="text" id="name" placeholder="*">

                    <label for="surname">Nazwisko</label>
                    <input type="text" id="surname" placeholder="*">

                    <label for="mail">E-mail</label>
                    <input type="email" id="mail" placeholder="*">
                </div>


                <div class="form-area">
                    <textarea placeholder="Write here..."></textarea>
                </div>
            </div>

            <button class="form-btn">Wyślij</button>
            <div class="map">
                <iframe
                    src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1907.2937224453271!2d19.023725345124475!3d47.464180641601025!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x4741dde44bf01955%3A0x5bfa697fb38129af!2sEtele%20Cinema!5e0!3m2!1spl!2spl!4v1676049399370!5m2!1spl!2spl"
                    width="550" height="350" style="border:0;" allowfullscreen loading="lazy"
                    referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>
        </div>


    </section>




    <!-- @include 'footer.kit' -->
    <script src="/dist/js/main.min.js"></script>
</body>

</html>